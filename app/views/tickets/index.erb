<h1>Tickets</h1>
<a href='/tickets/new'>New Ticket</a><br /><br />
<% @tickets.each do |ticket| %>
  <div>
    <a href='/tickets/<%=ticket.id%>'><%= ticket.name %></a> (<% if ticket.complete %>complete<% else %>open<% end %>)<br />
    Created on: <%= ticket.created_at.strftime("%Y-%m-%d, %I:%M%p") %><br /><br />
  </div>
<% end %>
